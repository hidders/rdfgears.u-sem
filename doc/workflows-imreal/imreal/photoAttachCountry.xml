<?xml version="1.0" encoding="UTF-8"?>
<rdfgears>
   <metadata>
      <id>imreal/photoAttachCountry</id>
      <description>given a photo record, attach the country to the picture metadata
INPUT:  [ &amp;lt;photo&amp;gt; ] 
OUTPUT: [ country: RDFValue, photo: [&amp;lt;photo&amp;gt;] ] </description>
      <password>semweb</password>
   </metadata>
   <workflow>
      <workflowInputList x="41" y="41">
         <workflowInputPort name="photo"/>
      </workflowInputList>
      <network output="node_13" x="771" y="282">
         <processor id="node_13" x="674" y="5">
            <function type="record-create">
               <config param="fields">photo;country;estimatedCountry;</config>
            </function>
            <inputPort iterate="false" name="photo">
               <source workflowInputPort="photo"/>
            </inputPort>
            <inputPort iterate="false" name="country">
               <source processor="node_205"/>
            </inputPort>
            <inputPort iterate="false" name="estimatedCountry">
               <source processor="node_167"/>
            </inputPort>
         </processor>
         <processor id="node_205" x="532" y="117">
            <function type="record-project">
               <config param="projectField">country</config>
            </function>
            <inputPort iterate="false" name="record">
               <source processor="node_11"/>
            </inputPort>
         </processor>
         <processor id="node_11" x="369" y="83">
            <function type="custom-java">
               <config param="implementation">nl.tudelft.rdfgears.rgl.function.imreal.CoordinatesToCountry</config>
            </function>
            <inputPort iterate="false" name="longitude">
               <source processor="node_10"/>
            </inputPort>
            <inputPort iterate="false" name="latitude">
               <source processor="node_9"/>
            </inputPort>
            <inputPort iterate="false" name="geonames_user">
               <source processor="node_12"/>
            </inputPort>
         </processor>
         <processor id="node_10" x="219" y="89">
            <function type="record-project">
               <config param="projectField">longitude</config>
            </function>
            <inputPort iterate="false" name="record">
               <source workflowInputPort="photo"/>
            </inputPort>
         </processor>
         <processor id="node_9" x="221" y="181">
            <function type="record-project">
               <config param="projectField">latitude</config>
            </function>
            <inputPort iterate="false" name="record">
               <source workflowInputPort="photo"/>
            </inputPort>
         </processor>
         <processor id="node_12" x="205" y="283">
            <function type="constant">
               <config param="value">"jasper.oosterman"</config>
            </function>
         </processor>
         <processor id="node_167" x="529" y="338">
            <function type="record-project">
               <config param="projectField">country</config>
            </function>
            <inputPort iterate="false" name="record">
               <source processor="node_164"/>
            </inputPort>
         </processor>
         <processor id="node_164" x="360" y="337">
            <function type="custom-java">
               <config param="implementation">nl.tudelft.rdfgears.rgl.function.imreal.CoordinatesToCountry</config>
            </function>
            <inputPort iterate="false" name="longitude">
               <source processor="node_166"/>
            </inputPort>
            <inputPort iterate="false" name="latitude">
               <source processor="node_165"/>
            </inputPort>
            <inputPort iterate="false" name="geonames_user">
               <source processor="node_12"/>
            </inputPort>
         </processor>
         <processor id="node_166" x="215" y="372">
            <function type="record-project">
               <config param="projectField">estimatedLong</config>
            </function>
            <inputPort iterate="false" name="record">
               <source workflowInputPort="photo"/>
            </inputPort>
         </processor>
         <processor id="node_165" x="216" y="472">
            <function type="record-project">
               <config param="projectField">estimatedLat</config>
            </function>
            <inputPort iterate="false" name="record">
               <source workflowInputPort="photo"/>
            </inputPort>
         </processor>
      </network>
   </workflow>
</rdfgears>
