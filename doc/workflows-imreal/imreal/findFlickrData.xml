<?xml version="1.0" encoding="UTF-8"?>
<rdfgears>
   <metadata>
      <id>imreal/findFlickrData</id>
      <description>Find and enrich flickr photo data</description>
      <password>semweb</password>
   </metadata>
   <workflow>
      <workflowInputList x="10" y="100">
         <workflowInputPort name="flickrUser"/>
		 <workflowInputPort name="twitterUser"/>
      </workflowInputList>
      <network output="node_43" x="394" y="359">
         <processor id="node_43" x="318" y="42">
            <function type="custom-java">
               <config param="implementation">nl.tudelft.rdfgears.rgl.function.imreal.FlickrPhotoLocator</config>
            </function>
            <inputPort iterate="false" name="flickrUser">
               <source workflowInputPort="flickrUser"/>
            </inputPort>
			 <inputPort iterate="false" name="twitterUser">
               <source workflowInputPort="twitterUser"/>
            </inputPort>
         </processor>
      </network>
   </workflow>
</rdfgears>
