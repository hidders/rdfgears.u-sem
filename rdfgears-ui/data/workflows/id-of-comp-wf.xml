<?xml version="1.0" encoding="UTF-8"?>
<rdfgears>
  <metadata>
    <id>id-of-comp-wf</id>
    <name>Sample of Complete WF</name>
    <description>this workflow is a sample of a complete workflow, contain many nodes and connected and also well typed</description>
    <category>ManyNodes</category>
    <password/>
  </metadata>
  <workflow>
    <workflowInputList x="280" y="154">
      <workflowInputPort name="input0">
        <type>
          <var name="a5"/>
        </type>
      </workflowInputPort>
      <workflowInputPort name="input1qwewr">
        <type>
          <var name="a6"/>
        </type>
      </workflowInputPort>
      <workflowInputPort name="input2">
        <type>
          <var name="a7"/>
        </type>
      </workflowInputPort>
    </workflowInputList>
    <network output="node-6" x="830" y="457">
      <output-type>
        <var name="a8"/>
      </output-type>
      <processor id="node-2" x="473" y="92">
        <function type="bagUnion">
          <config param="test">bababag</config>
        </function>
        <inputPort name="bag1" iterate="false">
          <source workflowInputPort="input0"/>
        </inputPort>
        <inputPort name="bag2" iterate="false">
          <source workflowInputPort="input1qwewr"/>
        </inputPort>
      </processor>
      <processor id="node-3" x="428" y="221">
        <function type="Categorizer">
          <config param="categorizer">abc</config>
          <config param="categories">cat1;cat2;</config>
        </function>
        <inputPort name="bag" iterate="false">
          <source workflowInputPort="input2"/>
        </inputPort>
      </processor>
      <processor id="node-4" x="639" y="132">
        <function type="function">
          <config param="implementation">def</config>
        </function>
        <inputPort name="opt2-data" iterate="false">
          <source processor="node-2"/>
        </inputPort>
      </processor>
      <processor id="node-5" x="884" y="196">
        <function type="sparqlQuery">
          <config param="bindVariables">input0;input1;</config>
          <config param="query">PREFIX abc: &lt;http://example.com/exampleOntology#&gt;
SELECT ?capital ?country
WHERE {
  ?x abc:cityname ?capital ;
     abc:isCapitalOf ?y .
  ?y abc:countryname ?country ;
     abc:isInContinent abc:Africa .
}</config>
        </function>
        <inputPort name="input0" iterate="false">
          <source processor="node-3"/>
        </inputPort>
        <inputPort name="input1" iterate="false">
          <source processor="node-4"/>
        </inputPort>
      </processor>
      <processor id="node-6" x="662" y="443">
        <function type="RecordProject"/>
        <inputPort name="record" iterate="false">
          <source processor="node-5"/>
        </inputPort>
      </processor>
    </network>
  </workflow>
</rdfgears>
